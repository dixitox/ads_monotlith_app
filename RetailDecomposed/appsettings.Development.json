    "MaxTokens": 800,
    "Temperature": 0.7
  },
  "AzureSearch": {
    "Endpoint": "https://retail-ai-search-service.search.windows.net",
    "IndexName": "products-index",
    "EmbeddingDeploymentName": "text-embedding-3-small"
